{
  "presets": [
    ["env", {
      "modules": false
    }],
    "react",
    "stage-0"
  ],
  "plugins": ["external-helpers"],
  "env": {
    "webpack": {
      "presets": [
        ["env", {
          "modules": false,
          "targets": {
            "browsers": ["last 2 versions", "ie >= 11"]
          }
        }],
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-runtime",
        "react-hot-loader/babel"
      ]
    },
    "test": {
      "presets": [
        "env",
        "react",
        "stage-0"
      ],
      "plugins": ["transform-runtime"]
    }
  }
}
